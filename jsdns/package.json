{
    "name": "js-dns",
    "private": false,
    "version": "0.0.1",
    "author": "root@iblog.pro",
    "description": "Simple DNS server configured using redis. Based on https://github.com/colmsjo/redis-dns",
    "homepage": "https://github.com/vorlon001",
    "license": "Apache-2.0",
    "preferGlobal": false,
    "bugs": {
        "url": "https://github.com/colmsjo/redis-dns/issues"
    },
    "main": "index.js",
    "bin": "./index.js",
    "scripts": {
        "dev": "export NODE_ENV='dev' && clear && node_modules/.bin/babel-node server.js",
        "start": "node_modules/.bin/babel-node server.js"
    },
    "dependencies": {
        "babel-cli": "^6.24.0",
        "babel-core": "^6.24.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-stage-2": "^6.22.0",
        "dnsd": "0.9.6",
        "express": "^4.15.3",
        "native-dns": "*",
        "native-dns-packet": "^0.1.1",
        "nconf": "*",
        "redis": "2.8.0",
        "sprint": "^0.3.1"
    },
    "devDependencies": {
        "xo": "^0.21.1"
    },
    "keywords": [
        "DNS",
        "bind",
        "dnsd",
        "redis"
    ],
    "xo": {
        "space": 4,
        "rules": {
            "space-before-function-paren": [
                "error",
                "never"
            ]
        },
        "envs": [
            "node",
            "es6"
        ]
    }
}
